{
	"name": "@bhc/canvas-react",
	"author": "bhc",
	"version": "0.1.0",
	"license": "MIT",
	"private": false,
	"source": "src/index.tsx",
	"main": "dist/index.js",
	"typings": "dist/index.d.ts",
	"types": "dist/index.d.ts",
	"files": [
		"dist",
		"src"
	],
	"engines": {
		"node": ">=10"
	},
	"scripts": {
		"start": "cross-env NODE_ENV=development webpack serve --config webpack.config.js --mode development",
		"prepublish": "tsc",
		"build:webpack": "cross-env NODE_ENV=production webpack --config webpack.config.js --mode production",
		"build:tsc": "tsc",
		"lint": "eslint src/*",
		"fix": "eslint src/* --fix",
		"size": "size-limit",
		"analyze": "size-limit --why",
		"serve": "parcel ./src/index.html"
	},
	"peerDependencies": {
		"react": ">=16"
	},
	"prettier": {
		"printWidth": 80,
		"semi": true,
		"singleQuote": true,
		"trailingComma": "es5"
	},
	"module": "dist/canvas-react.esm.js",
	"size-limit": [
		{
			"path": "dist/canvas-react.cjs.production.min.js",
			"limit": "10 KB"
		},
		{
			"path": "dist/canvas-react.esm.js",
			"limit": "10 KB"
		}
	],
	"peerDependencies": {
		"react": "^17.0.2",
		"react-dom": "^17.0.2"
	},
	"devDependencies": {
		"@babel/preset-react": "^7.16.0",
		"@babel/preset-typescript": "^7.16.0",
		"@size-limit/preset-small-lib": "^6.0.4",
		"@types/react": "^17.0.34",
		"@types/react-dom": "^17.0.11",
		"@typescript-eslint/eslint-plugin": "^5.3.1",
		"@typescript-eslint/parser": "^5.3.1",
		"babel-loader": "^8.2.3",
		"cross-env": "^7.0.3",
		"csstype": "^3.0.9",
		"eslint": "^6.8.0",
		"eslint-plugin-react": "^7.27.0",
		"html-webpack-plugin": "^5.5.0",
		"react": "^17.0.2",
		"react-dom": "^17.0.2",
		"size-limit": "^6.0.4",
		"ts-loader": "^9.2.6",
		"tslib": "^2.3.1",
		"typescript": "^4.4.4",
		"webpack": "^5.64.0",
		"webpack-cli": "^4.9.1",
		"webpack-dev-server": "^4.4.0"
	},
	"dependencies": {
	}
}
